
diag_data = open("Diagnostics_report.txt", "r")

a1 = 0
a2 = 0
a3 = 0
a4 = 0
a5 = 0
a6 = 0
a7 = 0
a8 = 0
a9 = 0
a10 = 0
a11 = 0
a12 = 0

b1 = 0
b2 = 0
b3 = 0
b4 = 0
b5 = 0
b6 = 0
b7 = 0
b8 = 0
b9 = 0
b10 = 0
b11 = 0
b12 = 0

# calculate gr_1

for x in diag_data:
    if x[1] == "1":
        a1 += 1
    elif x[1] == "0":
        b1 += 1

if a1 > b1:
    gr_1 = 1
else:
    gr_1 = 0

# calculate gr_2

for x in diag_data:
    if x[2] == "1":
        a2 += 1
    elif x[2] == "0":
        b2 += 1

if a2 > b2:
    gr_2 = 1
else:
    gr_2 = 0

