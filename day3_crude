    er_11 = 0
else:
    er_11 = 1

if gr_12 == 1:
    er_12 = 0
else:
    er_12 = 1

epsilon_rate_bin = (str(er_1) + str(er_2) + str(er_3) + str(er_4) + str(er_5) + str(er_6) + str(er_7) + str(er_8) +
                    str(er_9) + str(er_10) + str(er_11) + str(er_12))


def bin_2_num(binary):
    number = int(binary, 2)
    return number


gamma_rate_num = bin_2_num(gamma_rate_bin)
epsilon_rate_num = bin_2_num(epsilon_rate_bin)

power_consumption = gamma_rate_num * epsilon_rate_num

print("Your diagnostic report is ready, Commander.")

print("The Gamma Rate is: " + gamma_rate_bin)
print("The Epsilon Rate is: " + epsilon_rate_bin)

print("Current power consumption is: " + str(power_consumption))
