gamma_rate_bin = (str(gr_1) + str(gr_2) + str(gr_3) + str(gr_4) + str(gr_5) + str(gr_6) + str(gr_7) + str(gr_8) +
                  str(gr_9) + str(gr_10) + str(gr_11) + str(gr_12))

if gr_1 == 1:
    er_1 = 0
else:
    er_1 = 1

if gr_2 == 1:
    er_2 = 0
else:
    er_2 = 1

if gr_3 == 1:
    er_3 = 0
else:
